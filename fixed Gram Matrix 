        "import numpy as np\n",
        "\n",
        "def GramMatrix(atoms):\n",
        "  \n",
        "  G = np.zeros(shape=(len(atoms),len(atoms)))\n",
        "  atoms[5] = (0,0,0)\n",
        "  for i in range(len(atoms)):\n",
        "    for j in range(len(atoms)):\n",
        "      G[i,j] = G[j,i] = B[i+1,5] - B[i+1,j+1] + B[j+1,5]\n",
        "  return G\n",
        "\n",
        "#atoms[5] = (0,0,0)\n",
        "G =  GramMatrix(atoms.copy())\n",
        "print(G.round(1))\n",
        "\n"
